{
  "name": "sanity-plugin-shopify-assets",
  "version": "1.2.0",
  "description": "Choose assets from your Shopify store in your Sanity Studio",
  "keywords": [
    "sanity",
    "sanity-plugin",
    "images",
    "shopify",
    "assets",
    "source"
  ],
  "homepage": "https://github.com/sanity-io/sanity-plugin-shopify-assets#readme",
  "bugs": {
    "url": "https://github.com/sanity-io/sanity-plugin-shopify-assets/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sanity-io/sanity-plugin-shopify-assets.git"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js",
      "default": "./dist/index.esm.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "run-s clean && plugin-kit verify-package --silent && pkg-utils build --strict && pkg-utils --strict",
    "clean": "rimraf dist",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepublishOnly": "run-s build",
    "watch": "pkg-utils watch --strict"
  },
  "dependencies": {
    "@sanity/icons": "^3.7.0",
    "@sanity/incompatible-plugin": "^1.0.5",
    "@sanity/ui": "^2.15.2",
    "axios": "^1.2.2",
    "pretty-bytes": "^6.0.0",
    "pretty-ms": "^8.0.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-photo-album": "^2.0.0",
    "rxjs": "^7.8.0",
    "video.js": "^7.20.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@sanity/pkg-utils": "^2.2.1",
    "@sanity/plugin-kit": "^3.1.3",
    "@sanity/semantic-release-preset": "^3.0.2",
    "@types/react": "^18",
    "@types/video.js": "^7.3.50",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-sanity": "^6.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.2",
    "prettier-plugin-packagejson": "^2.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rimraf": "^4.0.4",
    "sanity": "^3.2.3",
    "styled-components": "^6.1.15",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "react": "^18.3 || ^19",
    "sanity": "^3 || ^4.0.0-0",
    "styled-components": "^6"
  },
  "engines": {
    "node": ">=14"
  }
}
